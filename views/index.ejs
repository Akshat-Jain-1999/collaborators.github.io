    <script>
      $('#upar').waypoint(function() {
          for(var i=0;i<$("#intro").parent().siblings().length;i++){
              $("#intro").parent().siblings()[i].children[0].classList.remove("active");
          }
          $('#intro').addClass("active");
      }, { offset: '10%'});

      $('#about').waypoint(function() {
